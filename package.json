    "bufferutil": "^4.0.7",
    "utf-8-validate": "^6.0.3"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/test/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/tests/**/*.test.js",
      "**/src/**/*.test.js"
    ],
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ],
    "testTimeout": 30000
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2022": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ],
      "prefer-const": "error",
      "no-var": "error"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 100,
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "BayneXSystem.js",
      "server.js"
    ],
    "ext": "js,json",
    "ignore": [
      "data/*",
      "logs/*",
      "backups/*",
      "node_modules/*",
      "coverage/*"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "delay": 1000
  },
  "config": {
    "port": 3000,
    "wsPort": 8080,
    "logLevel": "info"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/baynex-ax"
  },
  "cpu": [
    "x64",
    "arm64"
  ],
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "src/",
    "scripts/",
    "docs/",
    "BayneXSystem.js",
    "server.js",
    "package.json",
    "README.md",
    "LICENSE",
    ".env.template"
  ]
}
