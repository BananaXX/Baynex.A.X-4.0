{
  "name": "baynex-ax",
  "version": "1.0.0",
  "description": "BAYNEX.A.X - Binary Autonomous Yield Navigation & Execution X-System - Fully autonomous, self-learning, AI-powered multi-platform trading system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Tests will be implemented\" && exit 0",
    "build": "npm install",
    "postinstall": "echo 'BAYNEX.A.X dependencies installed successfully'"
  },
  "dependencies": {
    "express": "^4.19.2",
    "ws": "^8.17.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "axios": "^1.7.2",
    "node-fetch": "^3.3.2",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "uuid": "^9.0.1",
    "moment": "^2.30.1",
    "lodash": "^4.17.21",
    "dotenv": "^16.4.5",
    "chalk": "^5.3.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.1.0",
    "node-cron": "^3.0.3",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "joi": "^17.13.1",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "node-telegram-bot-api": "^0.66.0",
    "twilio": "^5.0.4",
    "technical-indicators": "^3.1.0",
    "brain.js": "^2.0.0-beta.2",
    "ml-matrix": "^6.10.9",
    "sqlite3": "^5.1.6",
    "better-sqlite3": "^9.4.5",
    "socket.io": "^4.7.5",
    "ioredis": "^5.4.1",
    "sharp": "^0.33.4"
  },
  "optionalDependencies": {
    "@tensorflow/tfjs-node": "^4.17.0",
    "puppeteer": "^22.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.3",
    "eslint": "^9.5.0",
    "prettier": "^3.3.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "trading",
    "autonomous",
    "ai",
    "machine-learning",
    "forex",
    "binary-options",
    "cryptocurrency",
    "algorithmic-trading",
    "fintech",
    "automation",
    "deriv",
    "metatrader",
    "iq-option",
    "profit-generation",
    "risk-management",
    "strategy-optimization",
    "real-time-trading",
    "multi-platform",
    "self-learning",
    "voice-assistant"
  ],
  "author": {
    "name": "BAYNEX Development Team",
    "email": "dev@baynex.ai"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BananaXX/Baynex.A.X-4.0.git"
  },
  "bugs": {
    "url": "https://github.com/BananaXX/Baynex.A.X-4.0/issues"
  },
  "homepage": "https://baynex.ai",
  "config": {
    "unsafe-perm": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@eslint/js/recommended"
    ],
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": "warn",
      "prefer-const": "error"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 4
  },
  "nodemonConfig": {
    "watch": [
      ".",
      "!logs/",
      "!data/",
      "!node_modules/"
    ],
    "ext": "js,json",
    "ignore": [
      "tests/",
      "coverage/",
      "docs/"
    ]
  }
}
