{
  "name": "baynex-ax",
  "version": "1.0.0",
  "description": "BAYNEX.A.X - Binary Autonomous Yield Navigation & Execution X-System. A fully autonomous, self-learning, AI-powered multi-platform trading system.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "prod": "NODE_ENV=production node server.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "setup": "node scripts/setup.js",
    "test-connections": "node scripts/test-connections.js",
    "backup": "node scripts/backup.js",
    "migrate": "node scripts/migration.js",
    "deploy": "node scripts/deploy.js",
    "logs": "tail -f logs/baynex-$(date +%Y-%m-%d).log",
    "health": "curl -f http://localhost:${PORT:-3000}/health || exit 1",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "clean": "rm -rf logs/* data/temp/* node_modules/.cache",
    "install:production": "npm ci --only=production",
    "prestart": "npm run setup",
    "postinstall": "npm run setup"
  },
  "keywords": [
    "trading",
    "autonomous",
    "ai",
    "machine-learning",
    "binary-options",
    "forex",
    "deriv",
    "mt5",
    "iq-option",
    "automated-trading",
    "fintech",
    "algorithmic-trading",
    "risk-management",
    "portfolio-management"
  ],
  "author": {
    "name": "BAYNEX.A.X Development Team",
    "email": "contact@baynex-ax.com",
    "url": "https://baynex-ax.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/baynex-ax/baynex-ax.git"
  },
  "bugs": {
    "url": "https://github.com/baynex-ax/baynex-ax/issues"
  },
  "homepage": "https://baynex-ax.com",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
